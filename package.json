{
    "private": true,
    "scripts": {
        "dev": "concurrently --kill-others-on-fail \"mix watch\" \"json-server --watch db.json\" \"php artisan serve\"",
        "development": "mix",
        "watch": "mix watch",
        "server": "json-server --watch db.json",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production"
    },
    "devDependencies": {
        "autoprefixer": "^10.4.0",
        "axios": "^0.24.0",
        "concurrently": "^7.0.0",
        "laravel-mix": "^6.0.6",
        "postcss": "^8.3.11",
        "sass": "^1.43.4",
        "sass-loader": "^12.1.0",
        "tailwindcss": "^2.2.19",
        "vue": "2.6.12",
        "vue-loader": "^15.9.7",
        "vue-template-compiler": "2.6.12",
        "vuex": "^3.6.2"
    },
    "dependencies": {
        "@tailwindcss/forms": "^0.3.4",
        "json-server": "^0.17.0",
        "uuid": "^8.3.2",
        "vue-js-modal": "^2.0.1",
        "vue-router": "^3.5.3"
    }
